                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT
    t.TipoTransferencia,
    CASE t.TipoTransferencia
        WHEN 'INTERNA' THEN 'Transferencia dentro del mismo banco'
        WHEN 'INTERBANCARIA' THEN 'Transferencia a otros bancos en Bolivia'
        WHEN 'INTERNACIONAL' THEN 'Transferencia internacional'
        ELSE 'Otro tipo de transferencia'
    END AS Descripcion
FROM operaciones.Transferencia t
LEFT JOIN  [BancoDW].[dbo].[dimTipoTransferencia] dtt
    ON t.TipoTransferencia = dtt.TipoTransferencia
WHERE dtt.TipoTransferencia IS NULL;</property>
