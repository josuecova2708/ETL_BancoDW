                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    cal.Fecha,
    cal.Dia,
    cal.Mes,
    cal.Anio,
    cal.NombreMes,
    cal.Trimestre,
    cal.Semestre,
    cal.NombreDiaSemana,
    cal.DiaSemana,
    cal.EsFinDeSemana
FROM dbo.fn_GenerarCalendario() cal
LEFT JOIN [BancoDW].[dbo].[DimTiempo] dt ON cal.Fecha = dt.Fecha
WHERE dt.Fecha IS NULL
ORDER BY cal.Fecha;</property>
