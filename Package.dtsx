    -- Foreign Keys para dimensiones
    tc.ClienteId,
    CAST(op.FechaSolicitud AS DATE) AS Fecha,
    op.MonedaId,
    op.CanalId,
    t.TipoTransferencia,
    
    -- Métricas
    op.Monto AS MontoTransferencia,
    AVG(op.Monto) OVER (PARTITION BY tc.ClienteId) AS MontoPromedioCliente
FROM operaciones.Transferencia t
INNER JOIN pagos.OrdenPago op 
    ON t.PaymentOrderId = op.PaymentOrderId
INNER JOIN nucleo.Cuenta cta 
    ON op.CuentaOrigenId = cta.CuentaId
INNER JOIN nucleo.TitularCuenta tc 
    ON cta.CuentaId = tc.CuentaId
-- 🔹 Se excluyen los registros ya cargados en el DW
LEFT JOIN [BancoDW].[dbo].[fact_transferencias] ft 
    ON t.TransferenciaId = ft.TransferenciaId
WHERE op.TipoOperacion = 'TRANSFER'
  AND op.EstadoOperacion = 'EJECUTADA'
  AND tc.RolTitular = 'OWNER'
  AND tc.Estado = 1
  AND ft.TransferenciaId IS NULL  -- ← excluye las transferencias ya existentes
ORDER BY op.FechaSolicitud;</property>
