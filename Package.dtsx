              description="Consume datos de SQL Server, OLE DB, ODBC u Oracle mediante el correspondiente proveedor de datos de .NET Framework. Se usa una instrucción Transact-SQL para definir el conjunto de resultados. Por ejemplo, se extraen datos de SQL Server con el proveedor de datos de .NET Framework para SQL Server."
              name="Origen de ADO NET"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    p.ProductoId,
    p.Codigo,
    p.Nombre,
    p.Categoria,
    CASE 
        WHEN p.Nombre LIKE '%Ahorro%' THEN 'Ahorro'
        WHEN p.Nombre LIKE '%Corriente%' THEN 'Corriente'
        WHEN p.Nombre LIKE '%Plazo Fijo%' THEN 'Plazo Fijo'
        WHEN p.Nombre LIKE '%Juvenil%' THEN 'Juvenil'
        WHEN p.Nombre LIKE '%Empresarial%' THEN 'Empresarial'
        WHEN p.Nombre LIKE '%Premium%' THEN 'Premium'
        WHEN p.Nombre LIKE '%Hipotecario%' THEN 'Hipotecario'
        WHEN p.Nombre LIKE '%Consumo%' THEN 'Consumo'
        WHEN p.Nombre LIKE '%Comercial%' THEN 'Comercial'
        WHEN p.Nombre LIKE '%Microcrédito%' THEN 'Microcrédito'
        WHEN p.Nombre LIKE '%Vehicular%' THEN 'Vehicular'
        WHEN p.Nombre LIKE '%Educativo%' THEN 'Educativo'
        ELSE 'Otro'
    END AS TipoProducto,
    m.Codigo AS MonedaProducto
FROM nucleo.Producto p
INNER JOIN referencial.Moneda m ON p.MonedaId = m.MonedaId
LEFT JOIN [BancoDW].[dbo].[DimProducto] dp ON p.ProductoId = dp.ProductoId
WHERE p.Estado = 1
  AND dp.ProductoId IS NULL  
ORDER BY p.ProductoId;</property>
